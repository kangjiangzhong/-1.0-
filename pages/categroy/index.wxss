page{
  height: 100%; 
}

.cates{
  height: 100%;
}
.cates_container{
  height: calc(100vh-90rpx);
  display: flex;
  width: 750rpx;
}
.leftMenu{
  flex: 2;
  height: 1334rpx;
}
.leftMenu .active{
  color: var(--themeColor);
  border-left: 5rpx solid currentColor;
}
.menuItem{
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.rightMenu{
  flex: 5;
  height: 1334rpx;
  
}
.cates_container .menuItem{
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-style: 30rpx;
  font-size:28rpx
}
.rightMenu .goodsGroup{
  margin-bottom: 25rpx;
}
.rightMenu .goodsGroup .goodsList{
  display: flex;
  flex-wrap: wrap;
 
}
.rightMenu .goodsGroup .goostitle{
  height: 80rpx;
  display: flex;
  
  justify-content: center;
  align-items: center;
 
}
.rightMenu .goodsGroup .goodsList navigator{
  padding: 10rpx;
  display: flex;
  flex-direction: column;
  width: 33.33%;
  text-align: center;
} 
.delimiter{
  color: #ccc;
  padding: 0 10rpx;
}
